{
  "compilerOptions": {
    "outDir": "./build/",
    "sourceMap": true,
    "strictNullChecks": true,
    "module": "es6",
    "resolveJsonModule": true,
    "jsx": "react",
    "target": "es5",
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": "./"
  },
  "include": ["./src/"],
  "exclude": ["node_modules"]
}
