{
  "compilerOptions": {
    "outDir": "./build/",
    "sourceMap": true,
    "strictNullChecks": true,
    "module": "ES2022",
    "resolveJsonModule": true,
    "jsx": "react",
    "target": "es5",
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["./src/"],
  "exclude": ["node_modules"]
}
